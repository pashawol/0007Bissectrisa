mixin s-gal-show-room()
	// start s-gal-show-room
	+b.SECTION.s-gal-show-room.section.lazy-background#s-gal-show-room&attributes(attributes)
		.container
			h5 В Шоу-руме Bissectrisa представлены все виды оконно-дверных порталов из алюминия
			+e.slider-wrapper
				+e.slider--js
					- for (var x = 0; x < 3; x++)
						+e.slide
							+e.img-wrap.bg-wrap.object-fit-js
								picture.picture-bg
									img.object-fit-js(src="#" data-lazy="img/@2x/Layer_179.jpg", alt="")
	// end s-gal-show-room